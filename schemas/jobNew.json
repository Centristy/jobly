{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://our.company.com/jobNew.schema.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 30
      },
      "salary": {
        "type": "integer"
      },

      "equity": {
        "type": "integer"
      },
      "company_handle": {
        "type": "string"
      }

    },
    "additionalProperties": false,
    "required": [
      "company_handle",
      "title"
    ]
  }
  